{
  "name": "fauxqs-benchmarks",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "bench:fauxqs-library": "node src/runners/fauxqs-library.ts",
    "bench:fauxqs-docker-official": "node src/runners/fauxqs-docker-official.ts",
    "bench:fauxqs-docker-lite": "node src/runners/fauxqs-docker-lite.ts",
    "bench:localstack": "node src/runners/localstack.ts",
    "bench:merge": "node src/merge-results.ts",
    "bench:clean": "del-cli results/*",
    "bench:all": "node --run bench:clean && npm run bench:fauxqs-library && npm run bench:fauxqs-docker-official && npm run bench:fauxqs-docker-lite && npm run bench:localstack && npm run bench:merge"
  },
  "dependencies": {
    "fauxqs": "file:..",
    "@aws-sdk/client-sqs": "^3.700.0"
  },
  "devDependencies": {
    "tinybench": "^3.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "del-cli": "^6.0.0"
  }
}

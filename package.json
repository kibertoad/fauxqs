{
  "name": "fauxqs",
  "version": "0.5.0",
  "description": "SNS/SQS emulator",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "node --watch src/server.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint && oxfmt src && tsc --noEmit",
    "lint:fix": "oxlint --fix && oxfmt --write src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aws",
    "sqs",
    "sns",
    "emulator",
    "mock",
    "localstack",
    "testing",
    "queue",
    "pubsub",
    "message-queue",
    "fake",
    "local"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kibertoad/fauxqs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/kibertoad/fauxqs",
  "bugs": {
    "url": "https://github.com/kibertoad/fauxqs/issues"
  },
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/app.d.ts",
        "default": "./dist/app.js"
      }
    }
  },
  "bin": {
    "fauxqs": "dist/server.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "author": "Igor Savin",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.992.0",
    "@aws-sdk/client-sqs": "^3.992.0",
    "fastify": "^5.7.4"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}

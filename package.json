{
  "name": "fauxqs",
  "version": "0.0.1",
  "description": "SNS/SQS emulator",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "node --watch src/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint",
    "format": "oxfmt --write src",
    "format:check": "oxfmt src"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "author": "Igor Savin",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.992.0",
    "@aws-sdk/client-sqs": "^3.992.0",
    "fastify": "^5.7.4"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
